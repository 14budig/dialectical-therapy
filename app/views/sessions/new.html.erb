
<div class="container">
  <div class="white-tainer">
    <div class="form-box">
      <h3>Sign In</h3>
      <%= simple_form_for @user, url: '/sessions' do |f| %>
        <%= f.input :user_name, :as => :string %>
        <%= f.input :password %>
        <%= f.button :submit, 'Sign In' %>
      <% end %>
    </div>
  </div>
</div>
